{
  "name": "httpbin-app",
  "version": "1.0.0",
  "description": "Example project for @modscleo4/midori web API framework.",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "ttsc",
    "clean": "tsc -b --clean",
