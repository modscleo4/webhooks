  "devDependencies": {
    "@types/node": "^18.8.5",
    "autocannon": "^7.10.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.9.5",
    "typescript-transform-paths": "^3.3.1"
  },
  "dependencies": {
    "@prisma/client": "^4.5.0",
    "dotenv": "^16.0.3",
    "midori": "github:modscleo4/midori",
